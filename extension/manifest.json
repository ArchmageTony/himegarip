{
	"manifest_version": 2,
	"name": "Himegari Rip",
	"description": "",
	"version": "0.0.1",
	"icons": {
		"16": "img/16.png",
		"64": "img/64.png",
		"128": "img/128.png"
	},
	"browser_action": {
		"default_icon": {
			"16": "img/16.png",
			"64": "img/64.png",
			"128": "img/128.png"
		},
		"default_title": "Himegari Rip"
	},
	"permissions": [
		"debugger",
		"*://pc-play.games.dmm.co.jp/*",
		"*://osapi.dmm.com/*"
	],
	"background": {
		"scripts": [
			"js/background.js"
		]
	},
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
